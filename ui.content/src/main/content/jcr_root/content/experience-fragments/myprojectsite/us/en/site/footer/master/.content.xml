                jcr:primaryType="nt:unstructured"
                sling:resourceType="myprojectsite/components/text"
                text="&lt;p>Copyright 2022, ProjectSite.&amp;nbsp;All rights reserved.&lt;/p>&#xd;&#xa;&lt;p>345 Park Avenue,&amp;nbsp;San Jose, CA 95110-2704, USA&lt;/p>&#xd;&#xa;"
